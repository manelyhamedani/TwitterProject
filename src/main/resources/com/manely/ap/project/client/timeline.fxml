<fx:root type="Group" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.manely.ap.project.client.controller.Timeline">
   <children>
      <ListView fx:id="timeLineListView" focusTraversable="false" prefHeight="400.0" prefWidth="530.0" />
      <Button fx:id="tweetButton" layoutX="460.0" layoutY="330.0" mnemonicParsing="false" onAction="#tweetButtonPressed" style="-fx-background-radius: 10em; -fx-min-width: 50px; -fx-min-height: 50px; -fx-max-width: 50px; -fx-max-height: 50px; -fx-background-color: #36b9ff;" text="+" textFill="WHITE">
         <font>
            <Font name="Apple Braille" size="25.0" />
         </font>
      </Button>
   </children>
</fx:root>
